    "def ConvertToTimeDelay(x,itr,offset):\n",
    "    c = 3*10**8\n",
    "    timeDelay = []\n",
    "    for i in range(0,len(x)):\n",
    "        timedelay = (x[i]*itr + offset)*25.4*2/(10**-12*c)\n",
    "        timeDelay.append(timedelay)\n",
    "    return(timeDelay)\n",
    "\n"
   ]
